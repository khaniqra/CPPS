Roll no:- FY20L1256
Problem statement: To check whether a number is prime or not.
*/

#include<stdio.h>
int main()
{
    int n;

    printf("Enter a number:");
    scanf("%d",&n);

    if(n==2||n==3||n==5||n==7)
    {
        printf("%d is a prime number.",n);
    }
    else if(n<0)
    {
        printf("Plz enter a positive number.");
    }
    else if(n%2==0)
    {
        printf("%d is not a prime number.",n);
    }
    else if(n%3==0)
    {
        printf("%d is not a prime number.",n);
    }
    else if(n%5==0)
    {
        printf("%d is not a prime number.",n);
    }
    else if(n%7==0)
    {
        printf("%d is not a prime number.",n);
    }
    else if(n==1||n==0)
    {
        printf("%d is not a prime or composite number.",n);
    }

    else
    {
        printf("%d is a prime number.",n);

    }
return 0;
}

